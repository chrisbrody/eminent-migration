{
  "format": "page",
  "id": "blog_page",
  "label": "Blog Page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "Blog Slug",
          "placeholder": ""
        }
      },
      "tagline": {
        "type": "Select",
        "config": {
          "label": "Tagline",
          "placeholder": "",
          "options": [
            "Bathroom Tips",
            "Kitchen Tips",
            "Interior Tips",
            "Living Tips",
            "Modern Tips",
            "Tips"
          ]
        }
      },
      "featured_image": {
        "type": "Image",
        "config": {
          "label": "Featured Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "headline": {
        "type": "Text",
        "config": {
          "label": "Headline",
          "placeholder": ""
        }
      },
      "short_description": {
        "type": "StructuredText",
        "config": {
          "label": "Short Description",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "Date",
          "placeholder": ""
        }
      },
      "time_to_read": {
        "type": "Number",
        "config": {
          "label": "Time to Read (minutes)",
          "placeholder": ""
        }
      },
      "owner": {
        "type": "Link",
        "config": {
          "label": "Owner",
          "select": "document",
          "customtypes": ["owner"]
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "call_to_action": {
              "type": "SharedSlice"
            },
            "faq": {
              "type": "SharedSlice"
            },
            "image_gallery": {
              "type": "SharedSlice"
            },
            "rich_text_content": {
              "type": "SharedSlice"
            },
            "hero": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}
