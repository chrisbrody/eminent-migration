{
  "id": "faq",
  "type": "SharedSlice",
  "name": "FAQ",
  "description": "FAQ",
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "docURL": "...",
      "version": "initial",
      "description": "Default",
      "primary": {
        "tagline": {
          "type": "Text",
          "config": {
            "label": "Tagline",
            "placeholder": "FAQ section"
          }
        },
        "title": {
          "type": "StructuredText",
          "config": {
            "label": "Title",
            "placeholder": "Frequently Asked Questions",
            "allowTargetBlank": true,
            "single": "heading1,heading2,heading3,heading4,heading5,heading6"
          }
        },
        "description": {
          "type": "StructuredText",
          "config": {
            "label": "Description", 
            "placeholder": "We've compiled the most important information to help you get the most out of your interior design experience. Can't find what you're looking for?",
            "allowTargetBlank": true,
            "single": "paragraph"
          }
        },
        "contact_link_text": {
          "type": "Text",
          "config": {
            "label": "Contact Link Text",
            "placeholder": "Contact us"
          }
        },
        "contact_link": {
          "type": "Link",
          "config": {
            "label": "Contact Link",
            "placeholder": "Select a page or enter URL",
            "allowTargetBlank": true
          }
        }
      },
      "items": {
        "question": {
          "type": "Text",
          "config": {
            "label": "Question",
            "placeholder": "Enter your question here..."
          }
        },
        "answer": {
          "type": "StructuredText",
          "config": {
            "label": "Answer",
            "placeholder": "Enter your answer here...",
            "allowTargetBlank": true,
            "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
          }
        }
      }
    }
  ]
}